<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button layoutX="23.0" layoutY="18.0" mnemonicParsing="false" onAction="#selectHashFile" text="Select hash file" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="10.0" />
      <Button layoutX="155.0" layoutY="18.0" mnemonicParsing="false" onAction="#clearHashes" text="Clear all hashes" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0" />
      <Label fx:id="lHashfileSize" layoutX="290.0" layoutY="23.0" prefHeight="16.0" prefWidth="274.0" text="Hashes loaded: 0" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="15.0" />
      <ListView fx:id="lvLoadedHashes" layoutX="19.0" layoutY="59.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="50.0" />
      <TextField fx:id="tfAddHash" layoutX="20.0" layoutY="428.0" onAction="#addHash" prefHeight="26.0" prefWidth="779.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="110.0" />
      <Button layoutX="818.0" layoutY="429.0" mnemonicParsing="false" onAction="#addHash" text="Add hash" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="20.0" />
   </children>
</AnchorPane>
